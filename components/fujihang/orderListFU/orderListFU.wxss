.orderList {
  font-size: 24rpx;
  color: #666;
  background: white;
  margin-bottom: 20rpx;
}
.orderList .orderListTop .name {
  flex: 1;
}
.orderList .orderListTop .name .red {
  color: #F56F68;
  margin-left: 10rpx;
}
.orderList .orderListTop .state {
  font-size: 28rpx;
  color: #F56F68;
}
.orderList .orderListTop .statusId {
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}
.orderList .orderListContent {
  background: #F7F7F7;
  padding: 20rpx 20rpx 10rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #f0f0f0;
}
.orderList .orderListContent .contant {
  padding-left: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: flex-start;
}
.orderList .orderListContent .contant image {
  width: 140rpx;
  height: 140rpx;
}
.orderList .orderListContent .contant .middin {
  flex: 1;
  margin-left: 20rpx;
}
.orderList .orderListContent .contant .middin .name_mid {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  display: flex;
}
.orderList .orderListContent .contant .middin .name_mid .name_xz {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
.orderList .orderListContent .contant .middin .name_mid .money {
  margin-left: 5rpx;
  flex: 1;
  text-align: right;
  color: #F56F68;
}
.orderList .orderListContent .contant .middin .teacher {
  color: #61B6FD;
  font-size: 28rpx;
  margin-bottom: 10rpx;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
.orderList .orderListContent .contant .middin .detail {
  color: #999;
  font-size: 28rpx;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
.orderList .orderListContent .contant .middin .detail .code {
  margin-right: 10rpx;
  border-radius: 6rpx;
  background: #F56F68;
  color: white;
  font-size: 26rpx;
  box-sizing: border-box;
  padding: 2rpx 6rpx;
}
.orderList .orderListContent .time {
  margin: 10rpx 0  10rpx 20rpx;
  color: #61B6FD;
  font-size: 24rpx;
}
.orderList .orderListAll {
  padding: 20rpx 20rpx 20rpx 0;
  margin-left: 20rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #999;
  border-bottom: 1px solid #f0f0f0;
  display: flex;
  align-items: center;
}
.orderList .orderListAll .moneyAll {
  color: #F56F68;
}
.orderList .orderListAll .btn {
  flex: 1;
  display: flex;
  justify-content: flex-end;
}
.orderList .orderListAll .btn .fuBtn {
  margin: 0;
  width: 160rpx;
  font-size: 24rpx;
  background: #F56F68;
  color: white ;
}
.orderList .iosJJ {
  margin: 0 40rpx 65%;
  background: white;
  border-radius: 15rpx;
  overflow: hidden;
}
.orderList .choosePay {
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f5f5f5;
}
.orderList .choosePay image {
  width: 100%;
  height: 70rpx;
}
