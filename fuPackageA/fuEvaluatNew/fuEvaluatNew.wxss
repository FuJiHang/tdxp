page {
  height: 100%;
  font-size: 26rpx;
  position: relative;
}
#fuEvaluatNew {
  color: #666;
}
#fuEvaluatNew .evaNew {
  margin-bottom: 20rpx;
  background: white;
  padding-bottom: 20rpx;
}
#fuEvaluatNew .evaNew textarea {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  height: 250rpx;
  border-top: 1px solid #f5f5f5;
}
#fuEvaluatNew .evaNew .chooselt {
  display: flex;
  align-items: center;
  padding: 0 10rpx;
  box-sizing: border-box;
}
#fuEvaluatNew .evaNew .chooselt .child {
  padding: 3rpx 30rpx;
  margin: 10rpx;
  border: 1px solid #f0f0f0;
  border-radius: 30rpx;
  color: #999;
  text-align: center;
  box-sizing: border-box;
}
#fuEvaluatNew .evaNew .chooselt .choose {
  border-color: #F56F68;
  color: #F56F68;
}
#fuEvaluatNew .evaNew .cate {
  padding: 0 20rpx ;
  box-sizing: border-box;
}
#fuEvaluatNew .evaNew .cate .name {
  font-size: 30rpx;
  font-weight: 800;
}
#fuEvaluatNew .evaNew .cate .child {
  display: flex;
  align-items: center;
  padding: 6rpx 0;
  box-sizing: border-box;
}
#fuEvaluatNew .evaNew .cate .child .van-rate {
  margin-left: 20rpx;
  margin-top: 10rpx;
}
#fuEvaluatNew .assem {
  padding: 20rpx;
  box-sizing: border-box;
  background: white;
}
#fuEvaluatNew .assem .childA {
  position: relative;
  margin-top: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
#fuEvaluatNew .assem .childA .proImg {
  height: 170rpx;
  width: 170rpx;
  border-radius: 20rpx;
  display: flex;
  justify-items: center;
  align-items: center;
  overflow: hidden;
}
#fuEvaluatNew .assem .childA .proImg image {
  background: #fff9f9;
  height: 100%;
}
#fuEvaluatNew .assem .childA .contant {
  flex: 1;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  height: 100%;
}
#fuEvaluatNew .assem .childA .contant .ptcg {
  top: 30%;
  right: 30rpx;
  position: absolute;
  width: 100rpx;
  height: 100rpx;
}
#fuEvaluatNew .assem .childA .contant .nameC {
  display: flex;
  align-items: center;
}
#fuEvaluatNew .assem .childA .contant .nameC .le {
  flex: 1;
  margin-right: 5rpx;
  font-weight: 800;
  color: #333;
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
#fuEvaluatNew .assem .childA .contant .nameC .ri {
  font-size: 28rpx;
  color: #F56F68;
}
#fuEvaluatNew .assem .childA .contant .detail {
  margin: 10rpx 0;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
  color: #999;
}
#fuEvaluatNew .assem .childA .contant .price {
  flex: 1;
  display: flex;
  align-items: flex-end;
  color: #999;
}
#fuEvaluatNew .assem .childA .contant .price .mon {
  color: #F56F68;
  font-size: 32rpx;
  flex: 1;
}
#fuEvaluatNew .assem .childA .contant .price .num {
  margin-right: 10rpx;
}
#fuEvaluatNew .assem .childA .contant .price .btn .van-button {
  border-radius: 30rpx;
  background: #F56F68;
  color: white;
}
#fuEvaluatNew .imgList {
  display: flex;
  align-items: center;
  padding: 10rpx ;
  box-sizing: border-box;
  flex-wrap: wrap;
  margin-bottom: 20rpx;
  border-bottom: 1px solid #f1f1f1;
}
#fuEvaluatNew .imgList .padding {
  padding: 10rpx ;
  box-sizing: border-box;
  width: 25%;
}
#fuEvaluatNew .imgList .choose {
  height: 180rpx;
  box-sizing: border-box;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  font-size: 30rpx;
  color: #999;
  background: #f5f5f5;
  border-radius: 15rpx;
}
#fuEvaluatNew .imgList .choose image {
  height: 100rpx;
  width: 100rpx;
  margin-bottom: 5rpx;
}
#fuEvaluatNew .imgList .img {
  padding: 10rpx ;
  box-sizing: border-box;
  width: 25%;
  position: relative;
}
#fuEvaluatNew .imgList .img image {
  border-radius: 15rpx;
  height: 170rpx;
  width: 100%;
}
#fuEvaluatNew .btn {
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 9;
}
#fuEvaluatNew .btn .van-button {
  width: 100%;
  border: none;
  border-radius: 0;
  color: white;
  background: #f56e68;
}
