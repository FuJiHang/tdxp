@import "../all.wxss";

.layout {
  padding-bottom: 28rpx;
  background-color: #fff;
}

.layoutPD {
  padding-left: 28rpx !important;
  padding-right: 28rpx !important;
}
.yuShou{
  padding:0 24rpx;
  font-weight: bold;
  color: #666;
}
.yuShou .price{
  margin: 24rpx 0 6rpx;
  font-size: 24rpx;
}
.yuShou .pri{
  margin-left: 24rpx;
  display: flex;
  align-items: baseline;
}
.yuShou .red{
  display: flex;
  align-items:baseline;
  color: #FB1F4F;
}
.yuShou .red .big{
  font-size: 48rpx;
}
.navTopG {
  padding: 16rpx 24rpx 0;
  font-size: 32rpx;
  font-weight: 500;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.kdNum {
  margin: 14rpx 24rpx 42rpx;
  display: flex;
  align-items: center;
  font-size: 22rpx;
  color: #666;

}

.kdNum .flex {
  flex: 1;
  text-align: center;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.kdNum .child {
  width: 30%;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.jzjs {
  padding-bottom: 40rpx;
  display: flex;
  align-items: center;
  background: rgb(255, 255, 255);
}

.jzjs .child {
  flex: 1;
  font-size: 22rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.jzjs .child image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 5rpx;
}

.redCard {
  margin: 0 24rpx ;
  padding: 16rpx 28rpx;
  background: linear-gradient(34deg, rgba(6, 0, 22, 1) 0%, rgba(15, 8, 32, 1) 100%);
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  font-weight: bold;
  background: url('http://hmqy.oss-cn-hangzhou.aliyuncs.com/tdhmd/VIP@3x.png') no-repeat;
  background-size: 100% 100%;
}

.redCard .name {
  color: #fff;
  font-size: 28rpx;
  flex: 1;
}

.redCard .buttn {
  width: 146rpx;
  height: 46rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 26rpx;
  border:none;
  color: #FB1F4F;
  font-size: 18rpx;
  text-align: center;
  line-height: 46rpx;
}

.miniF {
  font-size: 26rpx;
  color: #FB1F4F;
}

.bigF {
  font-size: 48rpx;
  color: #FB1F4F;
}

.goods-price {
  color: #f44;
  font-weight: bold;
  margin-right: 20rpx;
  font-size: 32rpx;
}

.goods-price text:first-child {
  font-size: 38rpx;
  margin-bottom: 10rpx;
}

.goods-price text:last-child {
  font-size: 64rpx;
}

.buyNum1 .newUserTxt {
  color: #fff;
  width: 131rpx;
  height: 25rpx;
  border-radius: 13rpx 12rpx 12rpx 0;
  background-color: #f44;
  font-size: 20rpx;
  margin-right: 10rpx;
}

.buyNum1 .buyNumTxt {
  color: #666;
  font-size: 26rpx;
  margin-top: 5rpx;
}

.buyNum1 .buyNumTxt text:first-child {
  margin-right: 20rpx;
}

.buyNum1.buyNum6>view:last-child text:last-child {
  text-decoration: none;
}


.buyNum2 {
  width: 310rpx;
  height: 44rpx;
  background-color: #FFE2CC;
  color: #FC8833;
  font-size: 24rpx;
  border-radius: 10rpx;
}


.goods-title {
  color: #333;
  font-size: 30rpx;
  box-sizing: border-box;
  padding: 0 0 10rpx;
  position: relative;
  display: flex;
  align-items: flex-end;
}

.goodsShare image {
  width: 30rpx;
  margin-bottom: 10rpx;

}

.goodsShare {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #999;
  position: relative;
}

.shareBtn {
  width: 60rpx;
  height: 83rpx;
  /* background-color: pink; */
  position: absolute;
  top: -1rpx;
  right: -2rpx;
  opacity: 0;
}

.goods-title>view:first-child {
  background-color: #f44;
  color: #fff;
  width: 100rpx;
  height: 34rpx;
}

.goods-titleL {
  background: linear-gradient(88deg, rgba(255, 51, 51, 0.94), rgba(255, 70, 70, 0.94));
  color: #fff;
  font-size: 22rpx;
  border-radius: 17rpx;
  width: 244rpx;
  height: 34rpx;
  display: inline-block;
  text-align: right;
  margin-bottom: 10rpx;
  margin-right: 10rpx;
}

.goods-titleL text {
  display: inline-block;
  width: 100rpx;
  height: 34rpx;
  border-radius: 17rpx;
  text-align: center;
  background: linear-gradient(0deg, rgba(255, 204, 153, 0.94), rgba(224, 169, 110, 0.94));
}

.goods-title .flex image {
  position: absolute;
  top: 26rpx;
  left: 24rpx;
  width: 105rpx;
}



.share_add {
  margin-right: 10rpx;
}

.detail {
  padding: 0 0 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #999;
}

.num_Buy {
  padding: 0 0 15rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #999;
}

.num_Buy .left {
  margin-right: 40rpx;
}


.fuXSQG {}

.fuXSQG .navTop {
  background: linear-gradient(135deg, rgba(255, 173, 112, 1), rgba(255, 142, 59, 1));
  display: flex;
  align-items: center;
  padding: 28rpx;
  box-sizing: border-box;
}

.fuXSQG .navTop .name {
  flex: 1;
  font-size: 32rpx;
  font-weight: bold;
  color: rgb(255, 87, 34);

}

.fuXSQG .navTop .val {
  display: flex;
  align-items: center;
}

.fuXSQG .navTop .val .child {
  height: 40rpx;
  line-height: 40rpx;
  width: 45rpx;
  text-align: center;
  color: #fff;
  background: rgba(51, 51, 51, 1);
  margin: 0 10rpx;
}

.fuXSQG .price_qg {
  display: flex;
  align-items: center;
  padding: 20rpx 28rpx 10rpx;
  box-sizing: border-box;
}

.fuXSQG .price_qg .name {
  font-size: 40rpx;
  color: #f44;
  font-weight: bold;
}

.fuXSQG .price_qg .name .mini {
  font-size: 24rpx;
}

.fuXSQG .price_qg .val {
  margin-left: 20rpx;
  color: #999;
  text-decoration: line-through;
}

.djs_price {
  padding: 10rpx 24rpx 12rpx;
  background: linear-gradient(90deg, rgba(255, 83, 83, 1) 0%, rgba(255, 43, 149, 1) 100%);
  color: #fff;
  display: flex;  
  
}
.djs {
  font-size: 22rpx;}

.djs  .time{
  display: flex;
  align-items: center;
}
.djs .child{
  margin:0 8rpx;
  padding: 0rpx 4rpx 1rpx;
  border-radius: 8rpx;
  background: #fff;
  color: #FB1F4F;
}
.djs  :first-child{
  margin-left: 0;
}

.djs_price .price {
  font-size: 48rpx;
  font-weight: bold;


}
.djs_price .flex {
  flex: 1;
  padding-top: 20rpx;
  margin: 0 5rpx;
  font-size: 26rpx;
  text-decoration: line-through;
}