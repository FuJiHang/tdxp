  .tabBar-box {
    width: 100%;
    position: fixed;
    bottom: 62rpx;
    padding: 0 29rpx;
    display: flex;
    justify-content: center;
    box-sizing: border-box;
  }
  
  .tabBar {
    position: relative;
    height: 98rpx;
    width: 507rpx;
    display: flex;
    flex: 1;
    vertical-align: top;
    background: linear-gradient(88deg, rgba(255, 51, 51, 1), rgba(255, 70, 70, 1));
    font-size: 20rpx;
    color: #8a8a8a;
    border-radius: 49rpx;
  }
  
  .tabBar-item {
    font-size: 24rpx;
    display: flex;
    justify-content: center;
    overflow: hidden;
    flex: 1;
    text-align: center;
  }
  
  .tabBar-item:nth-child(2)  .icon {
    width: 100rpx;
    height: 94rpx;
  }
  
