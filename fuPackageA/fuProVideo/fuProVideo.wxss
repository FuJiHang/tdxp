@keyframes fff {
  0% {
    transform: translateX(-100%);
    opacity: 0;
  }
  15% {
    transform: translateX(5%);
    opacity: 1;
  }
  17% {
    transform: translateX(-5%);
  }
  19% {
    transform: translateX(5%);
  }
  21% {
    transform: translateX(0%);
  }
  87% {
    transform: translateX(0%);
    opacity: 1;
  }
  100% {
    opacity: 0;
    transform: translateY(-100%);
    display: none;
  }
}
