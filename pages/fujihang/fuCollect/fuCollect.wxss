page {
  height: 100%;
  overflow: hidden;
}
#fuCollect {
  background: #f0f0f0;
  font-size: 26rpx;
  color: #666;
  height: 100%;
  overflow: hidden;
}
#fuCollect .tab {
  display: flex;
  align-items: flex-start;
  font-size: 30rpx;
  color: #333;
  padding: 20rpx 0;
  border-top: 1rpx solid #eee;
  background-color: #fff;
}
#fuCollect .tab .nav {
  flex: 1;
  text-align: center;
}
#fuCollect .tab .active {
  color: red;
  position: relative;
}
#fuCollect .tab .active::before {
  content: '';
  width: 70rpx;
  height: 7rpx;
  border-radius: 3rpx;
  background-color: red;
  position: absolute;
  left: 40%;
  bottom: -19rpx;
}
#fuCollect .colect {
  padding: 22rpx;
  background: #ffffff;
  box-sizing: border-box;
  margin-top: 19rpx;
  height: 100%;
  overflow: hidden;
}
#fuCollect .colect scroll-view {
  height: 100%;
  overflow: auto;
}
#fuCollect .containt {
  display: flex;
  border-bottom: 1px solid gainsboro;
  margin-bottom: 21rpx;
}
#fuCollect .containt > image {
  width: 164rpx;
  height: 164rpx;
}
#fuCollect .contr {
  padding-left: 16rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
#fuCollect .name {
  width: 414rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
#fuCollect .people {
  font-weight: 500;
  color: #666666;
  font-size: 22rpx;
}
#fuCollect .redue {
  width: 220rpx;
  min-width: 194rpx;
  background: #ffcccc;
  color: #ff3333;
  font-size: 20rpx;
}
#fuCollect .pricebox {
  display: flex;
  justify-content: space-between;
}
#fuCollect .price {
  font-size: 28rpx;
  font-weight: 800;
  color: #ff4444;
}
#fuCollect .emity {
  position: relative;
  font-size: 30rpx;
  text-align: center;
  top: 40%;
}
#fuCollect .contBL {
  height: 100%;
  overflow: hidden;
}
#fuCollect .contBL ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
#fuCollect .contBL scroll-view {
  height: 100%;
  overflow: auto;
}
#fuCollect .contBL scroll-view .productCont {
  padding: 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
#fuCollect .contBL scroll-view .product {
  background: white;
  margin: 7rpx;
  width: 350rpx;
  display: flex;
  flex-direction: column;
}
#fuCollect .contBL scroll-view .product .img {
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #fafafa;
}
#fuCollect .contBL scroll-view .product .img image {
  height: 220rpx;
  max-width: 100%;
}
#fuCollect .contBL scroll-view .product .name {
  font-size: 32rpx;
  padding: 10rpx 0;
  width: 340rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
#fuCollect .contBL scroll-view .product .det {
  color: #999;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
#fuCollect .contBL scroll-view .product .val {
  display: flex;
  align-items: center;
  color: #F56F68;
  padding: 0 20rpx;
  box-sizing: border-box;
}
#fuCollect .contBL scroll-view .product .val image {
  height: 30rpx;
  width: 30rpx;
}
#fuCollect .contBL scroll-view .product .val .ovFU {
  flex: 1;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
#fuCollect .contBL scroll-view .product .val .bea {
  font-size: 22rpx;
  overflow: hidden;
}
#fuCollect .contBL scroll-view .product .val .bea .num {
  margin: 0 10rpx;
  font-size: 30rpx;
}
#fuCollect .contBL scroll-view .product .val .xjin {
  font-size: 22rpx;
}
#fuCollect .contBL scroll-view .product .val .xjin .num {
  margin: 0 5rpx;
  font-size: 30rpx;
}
#fuCollect .contBL scroll-view .product .chaozuo {
  margin-top: 10rpx;
  border-top: 1rpx solid #fafafa;
  display: flex;
  align-items: center;
  width: 100%;
}
#fuCollect .contBL scroll-view .product .chaozuo .collect {
  justify-content: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  display: flex;
  filter: grayscale(100%);
  color: #F56F68;
  font-size: 28rpx;
}
#fuCollect .contBL scroll-view .product .chaozuo .collect .van-icon {
  margin-right: 10rpx;
}
#fuCollect .contBL scroll-view .product .chaozuo .coll {
  filter: grayscale(0);
}
#fuCollect .contBL scroll-view .product .chaozuo .btn {
  overflow: hidden;
  flex: 1;
}
#fuCollect .contBL scroll-view .product .chaozuo .btn .van-button {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  border: none;
  background: #F56F68;
  color: white;
  padding: 0;
  border-radius: 0;
}
#fuCollect .contBL scroll-view .product .chaozuo .btn .van-button .bImg {
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
}
#fuCollect .contBL scroll-view .product .btnFU {
  flex: 1;
  display: flex;
  align-items: flex-end;
  width: 100%;
}
#fuCollect .noMore {
  padding: 20rpx 20rpx 350rpx;
  text-align: center;
  font-size: 30rpx;
}
