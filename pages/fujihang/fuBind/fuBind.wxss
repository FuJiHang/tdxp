page {
  height: 100%;
  position: relative;
}
#fuBind {
  font-size: 26rpx;
  color: #666;
}
#fuBind .teach_name_tdz {
  padding: 0 20rpx;
  box-sizing: border-box;
  background: white;
  margin-bottom: 20rpx;
  font-size: 28rpx;
}
#fuBind .teach_name_tdz .teachName {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 1px solid #f0f0f0;
}
#fuBind .teach_name_tdz .teachName .name {
  width: 150rpx;
}
#fuBind .teach_name_tdz .teachName .child {
  flex: 1;
  margin-left: 20rpx;
}
#fuBind .bw {
  background: white;
}
#fuBind .navtop {
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
#fuBind .contont {
  font-size: 28rpx;
  padding: 20rpx 20rpx 20rpx 0;
  margin-left: 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f0f0f0;
  display: flex;
  flex-wrap: wrap;
}
#fuBind .contont .textfu {
  width: 100%;
  height: 200rpx;
}
#fuBind .contont textarea {
  box-sizing: border-box;
  width: 100%;
  font-size: 28rpx;
  max-height: 200rpx;
}
#fuBind .van-popup--bottom {
  max-height: 60%;
}
#fuBind .btn {
  width: 100%;
  position: absolute;
  bottom: 0;
}
#fuBind .btn .van-button {
  border: none;
  width: 100%;
  background: #F56F68;
  color: white;
}
#fuBind .seaConCL .chooseList {
  font-size: 30rpx;
  padding: 25rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  border-bottom: 1px solid #f0f0f0;
}
#fuBind .seaConCL .chooseList .name {
  margin-right: 10rpx;
  flex: 1;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
#fuBind .seaConCL .noMore {
  font-size: 28rpx;
  color: #999;
  padding: 20rpx;
  text-align: center;
  box-sizing: border-box;
}
#fuBind .seachMTAM {
  background: white;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}
#fuBind .seachMTAM .seachbg {
  margin-right: 10rpx;
  flex: 1;
  align-items: center;
  display: flex;
  background: #EEEEEE;
  border-radius: 40rpx;
}
#fuBind .seachMTAM .seachbg image {
  margin-left: 20rpx;
  height: 40rpx;
  width: 40rpx;
}
#fuBind .seachMTAM .seachbg input {
  font-size: 28rpx;
  margin: 0 20rpx;
  flex: 1;
  height: 60rpx;
}
#fuBind .seachMTAM .van-button {
  border: none;
  background: none;
}
