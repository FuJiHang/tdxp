page {
  height: 100%;
  overflow: hidden;
}
#fuCoupon {
  font-size: 26rpx;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  color: #666;
  box-sizing: border-box;
}
#fuCoupon ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
#fuCoupon .navTopMO {
  display: flex;
  padding: 18rpx 0;
  box-sizing: border-box;
  background: white;
}
#fuCoupon .navTopMO .child {
  flex: 1;
  text-align: center;
}
#fuCoupon .navTopMO .child .van-button {
  color: #999;
  border: none;
  background: white;
}
#fuCoupon .navTopMO .danger .van-button {
  background: #FF5353;
  color: white;
}
#fuCoupon .van_order {
  height: 100%;
  color: #666;
  overflow: hidden;
}
#fuCoupon .van_order .van-tabs {
  height: 100%;
  overflow: hidden;
}
#fuCoupon .van_order .van-tabs__content {
  height: 100%;
  overflow: hidden;
  padding-bottom: 44px;
  box-sizing: border-box;
}
#fuCoupon .van_order .van-tabs__content .van-tabs__track {
  height: 100%;
  overflow: hidden;
}
#fuCoupon .van_order .van-tabs__content .van-tabs__track .van-tab__pane {
  height: 100%;
  overflow: hidden !important;
}
#fuCoupon .van_order .van-tabs__content .van-tabs__track .van-tab__pane scroll-view {
  overflow: auto;
  height: 100%;
  background: #fff;
}
#fuCoupon .van_order .van-tabs__content .van-tabs__track .van-tab__pane scroll-view .noMore {
  font-size: 28rpx;
  color: #999;
  padding: 20rpx 20rpx 250rpx;
  text-align: center;
  box-sizing: border-box;
}
#fuCoupon .van_order .van-tab--active {
  color: red;
}
#fuCoupon .couponFu {
  margin: 10rpx 20rpx;
  box-sizing: border-box;
  padding: 0 20rpx 30rpx;
  background: #FF5353;
  position: relative;
  color: white;
  overflow: hidden;
}
#fuCoupon .couponFu .name_pic {
  position: relative;
  height: 110rpx;
  padding: 20rpx 0;
  margin-top: 5rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  z-index: 3;
  border-bottom: 1px dashed #f0f0f0;
}
#fuCoupon .couponFu .name_pic .name {
  font-size: 30rpx;
  flex: 1;
  margin-right: 10rpx;
}
#fuCoupon .couponFu .name_pic .pic {
  font-size: 32rpx;
}
#fuCoupon .couponFu .name_pic::before {
  background: #F7F7F7;
  content: '';
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
  position: absolute;
  bottom: -35rpx;
  left: -50rpx;
}
#fuCoupon .couponFu .name_pic::after {
  background: #F7F7F7;
  content: '';
  height: 70rpx;
  width: 70rpx;
  border-radius: 50%;
  position: absolute;
  bottom: -35rpx;
  right: -50rpx;
}
#fuCoupon .couponFu .rule {
  z-index: 3;
  position: relative;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
  font-size: 28rpx;
  margin: 20rpx 0;
}
#fuCoupon .couponFu .time {
  position: absolute;
  z-index: 3;
}
#fuCoupon .couponFu .time_hd {
  left: 20rpx;
  top: 5rpx;
}
#fuCoupon .couponFu .time_gq {
  right: 20rpx;
  bottom: 5rpx;
}
#fuCoupon .couponFu .bg {
  position: absolute;
  height: 100%;
  width: 120%;
  background: rgba(255, 255, 255, 0.15);
  left: -2%;
  top: 38%;
  transform: rotate(-17deg);
}
#fuCoupon .disableF {
  filter: grayscale(100%);
}
#fuCoupon .navTopMO {
  display: flex;
  padding: 18rpx 0;
  box-sizing: border-box;
  background: white;
}
#fuCoupon .navTopMO .child {
  flex: 1;
  text-align: center;
}
#fuCoupon .navTopMO .child .van-button {
  color: #999;
  border: none;
  background: white;
}
#fuCoupon .navTopMO .danger .van-button {
  background: #FF5353;
  color: white;
}
#fuCoupon .couponGan {
  margin: 10rpx 20rpx;
  box-sizing: border-box;
  padding: 30rpx 50rpx 30rpx 30rpx;
  background: #ff7171;
  position: relative;
  overflow: hidden;
}
#fuCoupon .couponGan .typeName {
  font-size: 24rpx;
  position: absolute;
  top: 15rpx;
  left: -50rpx;
  color: white;
  padding: 5rpx 50rpx;
  box-sizing: border-box;
  background: #f9b552;
  transform: rotate(-35deg);
}
#fuCoupon .couponGan .yuan {
  right: -35rpx;
  position: absolute;
  top: 0;
  display: flex;
  align-items: center;
  height: 100%;
}
#fuCoupon .couponGan .yuan .aaaa {
  border-radius: 50%;
  background: #F7F7F7;
  width: 70rpx;
  height: 70rpx;
}
#fuCoupon .couponGan .bWCou {
  background: white;
  position: relative;
  display: flex;
  align-items: center;
}
#fuCoupon .couponGan .bWCou .juchi {
  left: 0;
  overflow: hidden;
  position: absolute;
  min-width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
#fuCoupon .couponGan .bWCou .juchi .child {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #ff7171;
}
#fuCoupon .couponGan .bWCou .top {
  top: -5rpx;
}
#fuCoupon .couponGan .bWCou .btn {
  bottom: -5rpx;
}
#fuCoupon .couponGan .bWCou .left {
  min-height: 100%;
  left: -5rpx;
  top: 0;
  flex-direction: column;
  align-items: flex-start;
}
#fuCoupon .couponGan .bWCou .leftNaction {
  padding: 55rpx 30rpx 65rpx;
  box-sizing: border-box;
  font-size: 65rpx;
  color: #ff7171;
  font-weight: 700;
}
#fuCoupon .couponGan .bWCou .leftNaction span {
  font-size: 30rpx;
}
#fuCoupon .couponGan .bWCou .rightNaction {
  flex: 1;
  margin-left: 10rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}
#fuCoupon .couponGan .bWCou .rightNaction .name {
  font-size: 31rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
#fuCoupon .couponGan .bWCou .rightNaction .name span {
  background: linear-gradient(#fff, #ffeeee, #fff);
}
#fuCoupon .couponGan .bWCou .rightNaction .vip {
  color: #999;
  font-size: 29rpx;
  margin: 10rpx 0;
}
#fuCoupon .couponGan .bWCou .rightNaction .time {
  color: #999;
  font-size: 27rpx;
}
#fuCoupon .couponGanT {
  margin: 10rpx 20rpx;
  position: relative;
  overflow: hidden;
  display: flex;
}
#fuCoupon .couponGanT .letfCon {
  flex: 1;
  background: #FF5353;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  color: white;
  border-right: 1px dashed #fff;
  position: relative;
}
#fuCoupon .couponGanT .letfCon .contant {
  border-radius: 20rpx;
  border: 1px solid #fff;
  border-image: linear-gradient(to right, #fff, #FF5353) 1 10;
  padding: 20rpx;
  box-sizing: border-box;
  height: 100%;
  display: flex;
  flex-direction: column;
}
#fuCoupon .couponGanT .letfCon .name {
  font-size: 40rpx;
  font-weight: 700;
}
#fuCoupon .couponGanT .letfCon .name span {
  font-size: 30rpx;
}
#fuCoupon .couponGanT .letfCon .disp {
  display: flex;
  flex: 1;
  align-items: flex-end;
}
#fuCoupon .couponGanT .letfCon .role {
  flex: 1;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
  font-size: 28rpx;
  margin: 0 15rpx;
}
#fuCoupon .couponGanT .letfCon .time {
  border-left: 1px solid #f0f0f0;
  flex: 1;
  padding: 0 15rpx;
  box-sizing: border-box;
}
#fuCoupon .couponGanT .rightCon {
  padding: 15rpx;
  box-sizing: border-box;
  background: #FF5353;
  border-radius: 10rpx;
  width: 33.33%;
}
#fuCoupon .couponGanT .rightCon .contantR {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 100%;
  text-align: center;
  border-radius: 10rpx;
  background: white;
  position: relative;
}
#fuCoupon .couponGanT .rightCon .contantR .cp_btn {
  width: 210rpx;
  height: 185rpx;
  opacity: 0;
  position: absolute;
  top: 10rpx;
  right: 20rpx;
}
#fuCoupon .couponGanT .rightCon .buyName {
  padding: 10rpx 0 30rpx;
  color: #FF5353;
  font-size: 32rpx;
}
#fuCoupon .couponGanT .rightCon .noPad {
  padding-bottom: 0;
}
#fuCoupon .couponGanT .rightCon .pirce {
  font-size: 35rpx;
  margin: 8rpx 0 16rpx;
}
#fuCoupon .couponGanT .rightCon .btn {
  margin: 0 20rpx;
  padding: 5rpx 30rpx;
  box-sizing: border-box;
  background: #FF5353;
  color: white;
  border-radius: 20rpx;
}
#fuCoupon .couponGanT .yuan {
  position: absolute;
  height: 40rpx;
  width: 40rpx;
  border-radius: 40%;
  right: -20rpx;
  background: #fff;
}
#fuCoupon .couponGanT .glass {
  background: #fff;
  opacity: 0.2;
  width: 300rpx;
  height: 600rpx;
  position: absolute;
  top: -100rpx;
  left: -280rpx;
  transform: rotateZ(32deg);
}
#fuCoupon .couponGanT .xia {
  bottom: -30rpx;
}
#fuCoupon .couponGanT .shang {
  top: -30rpx;
}
#fuCoupon .iosJJ {
  margin: 0 10% 75%;
  background: white;
  border-radius: 15rpx;
  overflow: hidden;
}
#fuCoupon .iosJJ .child {
  display: flex;
  align-items: center;
  padding: 20rpx 20rpx 0;
