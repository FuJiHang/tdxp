  box-sizing: border-box;
  flex: 1;
  margin-left: 10rpx;
  border: 1px solid #f0f0f0;
}
#fuCoupon .iosJJ .btn {
  display: flex;
  align-items: center;
  padding: 20rpx 20rpx;
  box-sizing: border-box;
}
#fuCoupon .iosJJ .btn .flex {
