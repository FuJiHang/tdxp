}
#wjx_content #fuWriteOff .storeApp {
  background: white;
  margin-bottom: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
#wjx_content #fuWriteOff .storeApp image {
  height: 120rpx;
  width: 120rpx;
}
#wjx_content #fuWriteOff .storeApp .contant {
  flex: 1;
  margin-left: 20rpx;
}
#wjx_content #fuWriteOff .storeApp .contant .name_juli {
  font-size: 30rpx;
  display: flex;
  align-items: center;
}
#wjx_content #fuWriteOff .storeApp .contant .name_juli .juli {
  font-size: 28rpx;
  flex: 1;
