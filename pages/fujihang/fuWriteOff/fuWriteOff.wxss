}
#wjx_content #fuWriteOff .navTopP {
  background: white;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f0f0f0;
}
#wjx_content #fuWriteOff .navTopP image {
  height: 30rpx;
  width: 30rpx;
}
#wjx_content #fuWriteOff .navTopP .name {
  font-weight: bold;
  font-size: 30rpx;
  margin-left: 20rpx;
}
#wjx_content #fuWriteOff .Project {
