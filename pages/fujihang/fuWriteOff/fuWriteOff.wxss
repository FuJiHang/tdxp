}
#wjx_content #fuWriteOff .Project .cont_nav .van-icon {
  margin-top: 10rpx;
}
#wjx_content #fuWriteOff .Project .cont_det {
  margin: 15rpx 20rpx 0 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  align-items: center;
  background: #f8f8f8;
  display: flex;
}
#wjx_content #fuWriteOff .Project .cont_det image {
  height: 130rpx;
  width: 130rpx;
}
#wjx_content #fuWriteOff .Project .cont_det .detail {
  margin-left: 20rpx;
