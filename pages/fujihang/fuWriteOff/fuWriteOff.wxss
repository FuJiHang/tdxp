page {
  height: 100%;
  overflow: hidden;
  position: relative;
}
#wjx_content {
  box-sizing: border-box;
}
#wjx_content .header {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
}
#wjx_content .header .header_title {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}
#wjx_content .header .active {
  color: #E9696A;
  position: relative;
}
#wjx_content .header .active::after {
  content: '';
  width: 80rpx;
  height: 3rpx;
  background-color: #E9696A;
  position: absolute;
  left: 39%;
  bottom: -17rpx;
}
#wjx_content .order_wrap {
  box-sizing: border-box;
}
#wjx_content .order_wrap .order_search {
  display: flex;
  align-items: center;
  padding: 15rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
#wjx_content .order_wrap .order_search .order_time {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
#wjx_content .order_wrap .order_search .order_time .time_wrap {
  font-size: 26rpx;
  color: #333;
}
#wjx_content .order_wrap .order_search .order_time .time_img {
  width: 18rpx;
  height: 13rpx;
  margin-top: 10rpx;
}
#wjx_content .order_wrap .order_search .seach_input {
  display: flex;
  align-items: center;
  background-color: #eee;
  padding: 5rpx 15rpx;
  border-radius: 50rpx;
  box-sizing: border-box;
  width: 440rpx;
}
#wjx_content .order_wrap .order_search .seach_input .input_img {
  width: 36rpx;
  height: 36rpx;
}
#wjx_content .order_wrap .order_search .seach_input .in_data {
  width: 300rpx;
  padding: 0 10rpx;
}
#wjx_content .order_wrap .order_search .seach_input .pla_cla {
  font-size: 26rpx;
  color: #999;
}
#wjx_content .order_wrap .order_search .chazhao {
  flex: 0.8;
  font-size: 30rpx;
  color: #333;
  text-align: center;
}
#wjx_content .order_wrap .wjx_main {
  background-color: #fff;
  font-size: 26rpx;
  color: #999;
  padding: 15rpx;
}
#wjx_content .order_wrap .wjx_main .main_ren {
  display: flex;
  align-items: center;
  margin: 15rpx 0;
}
#wjx_content .order_wrap .wjx_main .main_ren .yyrs .r_num {
  margin-right: 50rpx;
}
#wjx_content .order_wrap .subject_wrap {
  display: flex;
  align-items: center;
  padding: 15rpx 25rpx 15rpx 15rpx;
  box-sizing: border-box;
}
#wjx_content .order_wrap .subject_wrap .subject_img {
  width: 80rpx;
  height: 80rpx;
  margin-right: 15rpx;
  border-radius: 10rpx;
}
#wjx_content .order_wrap .subject_wrap .subject_test {
  font-size: 28rpx;
  color: #333;
}
#wjx_content .order_wrap .show_result {
  background-color: #fff;
  font-size: 30rpx;
  color: #333;
  display: flex;
  flex-direction: column;
  font-weight: 600;
  align-items: flex-end;
  padding: 25rpx;
  border-bottom: 1rpx solid #eee;
}
#wjx_content .order_wrap .show_result .result_wrap {
  width: 222rpx;
  display: flex;
  justify-content: flex-start;
}
#wjx_content .order_wrap .payment {
  background-color: #fff;
  padding: 15rpx 25rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
}
#wjx_content .order_wrap .payment .payment_btn {
  border: 1rpx solid #E7483F;
  font-size: 30rpx;
  color: #E7483F;
  border-radius: 30rpx;
  padding: 10rpx 0;
  text-align: center;
  width: 160rpx;
}
#wjx_content #fuWriteOff {
  background-color: #fff;
  font-size: 26rpx;
  color: #666;
  height: 100%;
  overflow: auto;
  box-sizing: border-box;
  padding: 150rpx 0 50rpx 0;
}
#wjx_content #fuWriteOff .new_wjx {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
#wjx_content #fuWriteOff .new_wjx .soayisao {
  width: 660rpx;
  height: 88rpx;
  border: 1px solid #f0f0f0;
  font-size: 28rpx;
  border-radius: 50rpx;
  text-align: center;
  line-height: 88rpx;
  background: #F8F8F8;
  margin-bottom: 40rpx;
}
#wjx_content #fuWriteOff .new_wjx .soayisao::placeholder {
  color: #CCCCCC;
}
#wjx_content #fuWriteOff .new_wjx .hxInput {
  width: 660rpx;
  height: 88rpx;
  border: 1px solid #f0f0f0;
  font-size: 28rpx;
  border-radius: 50rpx;
  text-align: center;
  background: #F8F8F8;
}
#wjx_content #fuWriteOff .new_wjx .hxInput ::placeholder {
  color: #CCCCCC;
}
#wjx_content #fuWriteOff .new_wjx .img {
  display: flex;
  justify-content: center;
}
#wjx_content #fuWriteOff .new_wjx .img image {
  height: 50rpx;
}
#wjx_content #fuWriteOff .new_wjx .btn {
  width: 660rpx;
  box-sizing: border-box;
  border-radius: 50rpx;
}
#wjx_content #fuWriteOff .new_wjx .btn .van-button {
  width: 100%;
  background: #E65757;
  color: white;
  border-radius: 50rpx;
}
#wjx_content #fuWriteOff .Qcode {
  display: flex;
  justify-content: center;
}
#wjx_content #fuWriteOff .Qcode image {
