page {
  height: 100%;
  overflow: hidden;
}
#fuAchieve {
  font-size: 26rpx;
  color: #666;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
#fuAchieve .van_order {
  flex: 1;
  color: #666;
  overflow: hidden;
}
#fuAchieve .van_order .van-tabs {
  height: 100%;
  overflow: hidden;
}
#fuAchieve .van_order .van-tabs__content {
  height: 100%;
  overflow: hidden;
  padding-bottom: 44px;
  box-sizing: border-box;
}
#fuAchieve .van_order .van-tabs__content .van-tabs__track {
  height: 100%;
  overflow: hidden;
}
#fuAchieve .van_order .van-tabs__content .van-tabs__track .van-tab__pane {
  height: 100%;
  overflow: hidden !important;
}
#fuAchieve .van_order .van-tabs__content .van-tabs__track .van-tab__pane scroll-view {
  height: 100%;
}
#fuAchieve .van_order .van-tabs__content .van-tabs__track .van-tab__pane scroll-view .noMore {
  font-size: 28rpx;
  color: #999;
  padding: 20rpx;
  text-align: center;
  box-sizing: border-box;
}
#fuAchieve .van_order .van-tab--active {
  color: red;
}
#fuAchieve .btnDay {
  padding: 10rpx;
  box-sizing: border-box;
  background: #f56e68;
  color: white;
  display: flex;
  align-items: center;
  width: 100%;
  text-align: center;
}
#fuAchieve .btnDay .child {
  width: 50%;
  font-size: 36rpx;
}
#fuAchieve .btnDay .last {
  border-left: 1px solid #fd9f9a;
  width: 50%;
  padding: 2rpx 20rpx 2rpx 40rpx;
  margin: 18rpx 0;
  box-sizing: border-box;
}
#fuAchieve .btnDay .last .zon {
  margin-bottom: 5rpx;
  font-size: 28rpx;
}
