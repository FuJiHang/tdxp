page {
  height: 100%;
  overflow: hidden;
}
#fuWithdrawal {
  font-size: 26rpx;
  color: #666;
  height: 100%;
  overflow: hidden;
  padding: 20rpx;
  box-sizing: border-box;
  position: relative;
}
#fuWithdrawal .money {
  font-size: 36rpx;
  color: #F56F68;
  margin: 5% 0 20rpx;
}
#fuWithdrawal .topName {
  margin: 0 0 10%;
  font-size: 32rpx;
  color: #999;
  font-weight: 800;
}
#fuWithdrawal .withdra {
  padding: 20rpx;
  box-sizing: border-box;
  background: white;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
  position: relative;
  z-index: 3;
}
#fuWithdrawal .withdra .logo {
  height: 120rpx;
  width: 100%;
}
#fuWithdrawal .withdra .inputXx {
  padding: 0 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
#fuWithdrawal .withdra .inputXx .flex {
  padding: 20rpx 0;
  box-sizing: border-box;
  font-size: 30rpx;
  display: flex;
}
#fuWithdrawal .withdra .inputXx .flex .val {
  flex: 1;
  margin-left: 20rpx;
  border-bottom: 1px solid #f3f3f3;
}
#fuWithdrawal .withdra .inputXx .flex .val input {
  font-size: 28rpx;
  width: 100%;
}
#fuWithdrawal .textArea {
  background: white;
  padding: 20rpx ;
  box-sizing: border-box;
  display: flex;
}
#fuWithdrawal .textArea input {
  flex: 1;
}
#fuWithdrawal .withdra::after {
  z-index: 1;
  position: absolute;
  left: 10%;
  width: 80%;
  bottom: 0rpx;
  content: "";
  text-align: center;
  display: flex;
  height: 1rpx;
  box-shadow: 0 10rpx 60rpx #050505;
}
#fuWithdrawal .btn {
  bottom: 0;
  left: 0;
  width: 100%;
  position: absolute;
}
#fuWithdrawal .btn .van-button {
  border: none;
  width: 100%;
  background: #F56F68;
  color: white;
}
