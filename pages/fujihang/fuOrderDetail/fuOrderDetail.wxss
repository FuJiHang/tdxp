page {
  height: 100%;
  overflow: hidden;
}
#fuOrderDetail {
  font-size: 26rpx;
  color: #666;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
#fuOrderDetail .xfj {
  background: white;
  padding: 20rpx;
  margin: 20rpx 0;
  box-sizing: border-box;
  align-items: center;
  display: flex;
  font-size: 28rpx;
}
#fuOrderDetail .xfj .name {
  flex: 1;
}
#fuOrderDetail .contantOD {
  flex: 1;
  overflow: auto;
}
#fuOrderDetail .contantOD .navTopOD {
  background: white;
  padding: 20rpx 20rpx 0 0;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #999;
}
#fuOrderDetail .contantOD .navTopOD .bW {
  padding-bottom: 20rpx;
  box-sizing: border-box;
  margin-left: 20rpx;
  border-bottom: 1px solid #f0f0f0;
}
#fuOrderDetail .contantOD .navTopOD .orderId {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}
#fuOrderDetail .contantOD .navTopOD .orderId .val {
  margin-left: 30rpx;
  flex: 1;
}
#fuOrderDetail .contantOD .navTopOD .orderId .state {
  color: #43CC5A;
}
#fuOrderDetail .contantOD .navTopOD .payTime .val {
  margin-left: 30rpx;
}
#fuOrderDetail .contantOD .appoint {
  background: white;
  box-sizing: border-box;
  padding-bottom: 0.01rpx;
}
#fuOrderDetail .contantOD .appoint .navChild {
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
}
#fuOrderDetail .contantOD .appoint .navChild .name {
  flex: 1;
  font-size: 31rpx;
  font-weight: bold;
}
#fuOrderDetail .contantOD .appoint .navChild .time {
  color: #61B6FD;
}
#fuOrderDetail .contantOD .appoint .appointList {
  background: #f8f8f8;
  padding: 20rpx 20rpx 10rpx;
  display: flex;
  margin: 0 20rpx 20rpx;
  box-sizing: border-box;
}
#fuOrderDetail .contantOD .appoint .appointList .bW {
  background: white;
  height: 20rpx;
}
#fuOrderDetail .contantOD .appoint .appointList image {
  height: 140rpx;
  width: 140rpx;
}
#fuOrderDetail .contantOD .appoint .appointList .contant {
  flex: 1;
  margin-left: 20rpx;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .mB {
  font-size: 28rpx;
  margin-bottom: 10rpx;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .name_mon {
  margin-bottom: 20rpx !important;
  display: flex;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .name_mon .name {
  font-weight: bold;
  font-size: 30rpx;
  flex: 1;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .name_mon .mon {
  margin-left: 5rpx;
  font-size: 30rpx;
  color: #F56F68;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .address {
  color: #999999;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .code {
  color: #F56F68;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .disAC {
  display: flex;
  align-items: center;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .img {
  margin-right: 8rpx;
  height: 35rpx;
  width: 35rpx;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .time {
  display: flex;
  align-items: center;
  color: #61B6FD;
  margin-bottom: 10rpx;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .time .upgrade {
  flex: 1;
}
#fuOrderDetail .contantOD .appoint .appointList .contant .time .van-button {
  background: #F56F68;
  color: white;
}
#fuOrderDetail .contantOD .info {
  background: white;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
#fuOrderDetail .contantOD .info .navChild {
  font-size: 31rpx;
  font-weight: bold;
  padding: 0 20rpx 20rpx;
  box-sizing: border-box;
}
#fuOrderDetail .contantOD .info .childList {
  color: #999;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 0 20rpx 8rpx;
  display: flex;
}
#fuOrderDetail .contantOD .info .childList .name {
  width: 120rpx;
}
#fuOrderDetail .contantOD .info .childList .val {
  margin-left: 30rpx;
}
#fuOrderDetail .couponPE {
  padding: 20rpx;
  box-sizing: border-box;
  background: white;
  margin: 20rpx 0;
  display: flex;
  align-items: center;
}
#fuOrderDetail .couponPE .name {
  flex: 1;
  font-size: 28rpx;
}
#fuOrderDetail .couponPE .val {
  margin: 0 10rpx;
  color: #999;
}
#fuOrderDetail .couponPE .van-icon {
  margin-top: 5rpx;
}
#fuOrderDetail .couponGan {
  margin: 10rpx 20rpx;
  box-sizing: border-box;
  padding: 30rpx 50rpx 30rpx 30rpx;
  background: #ff7171;
  position: relative;
  overflow: hidden;
}
#fuOrderDetail .couponGan .typeName {
  font-size: 24rpx;
  position: absolute;
  top: 15rpx;
  left: -50rpx;
  color: white;
  padding: 5rpx 50rpx;
  box-sizing: border-box;
  background: #f9b552;
  transform: rotate(-35deg);
}
#fuOrderDetail .couponGan .yuan {
  right: -35rpx;
  position: absolute;
  top: 0;
  display: flex;
  align-items: center;
  height: 100%;
}
#fuOrderDetail .couponGan .yuan .aaaa {
  border-radius: 50%;
  background: #F7F7F7;
  width: 70rpx;
  height: 70rpx;
}
#fuOrderDetail .couponGan .bWCou {
  background: white;
  position: relative;
  display: flex;
  align-items: center;
}
#fuOrderDetail .couponGan .bWCou .juchi {
  left: 0;
  overflow: hidden;
  position: absolute;
  min-width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
#fuOrderDetail .couponGan .bWCou .juchi .child {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #ff7171;
}
#fuOrderDetail .couponGan .bWCou .top {
  top: -5rpx;
}
#fuOrderDetail .couponGan .bWCou .btn {
  bottom: -5rpx;
}
#fuOrderDetail .couponGan .bWCou .left {
  min-height: 100%;
  left: -5rpx;
  top: 0;
  flex-direction: column;
  align-items: flex-start;
}
#fuOrderDetail .couponGan .bWCou .leftNaction {
  padding: 55rpx 30rpx 65rpx;
  box-sizing: border-box;
  font-size: 65rpx;
  color: #ff7171;
  font-weight: 700;
}
#fuOrderDetail .couponGan .bWCou .leftNaction span {
  font-size: 30rpx;
}
#fuOrderDetail .couponGan .bWCou .rightNaction {
  flex: 1;
  margin-left: 10rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}
#fuOrderDetail .couponGan .bWCou .rightNaction .name {
  font-size: 31rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
#fuOrderDetail .couponGan .bWCou .rightNaction .name span {
  background: linear-gradient(#fff, #ffeeee, #fff);
}
#fuOrderDetail .couponGan .bWCou .rightNaction .vip {
  color: #999;
  font-size: 29rpx;
  margin: 10rpx 0;
}
#fuOrderDetail .couponGan .bWCou .rightNaction .time {
  color: #999;
  font-size: 27rpx;
}
#fuOrderDetail .couponGanT {
  margin: 10rpx 20rpx;
  position: relative;
  overflow: hidden;
  display: flex;
}
#fuOrderDetail .couponGanT .letfCon {
  flex: 1;
  background: #FF6262;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  color: white;
  border-right: 1px dashed #fff;
}
#fuOrderDetail .couponGanT .letfCon .contant {
  border-radius: 10rpx;
  border: 1px solid #fff;
  padding: 20rpx;
  box-sizing: border-box;
  height: 100%;
  display: flex;
  flex-direction: column;
}
#fuOrderDetail .couponGanT .letfCon .name {
  text-align: center;
  font-size: 40rpx;
  font-weight: 700;
}
#fuOrderDetail .couponGanT .letfCon .name span {
  font-size: 30rpx;
}
#fuOrderDetail .couponGanT .letfCon .role {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
  font-size: 28rpx;
  margin: 10rpx 10rpx 5rpx;
}
#fuOrderDetail .couponGanT .letfCon .time {
  flex: 1;
  display: flex;
  align-items: flex-end;
  justify-content: flex-end;
}
#fuOrderDetail .couponGanT .rightCon {
  padding: 15rpx;
  box-sizing: border-box;
  background: #FF6262;
  border-radius: 10rpx;
  width: 33.33%;
}
#fuOrderDetail .couponGanT .rightCon .contantR {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 100%;
  text-align: center;
  border-radius: 10rpx;
  background: white;
}
#fuOrderDetail .couponGanT .rightCon .buyName {
  padding-top: 10rpx;
  color: #999;
  font-size: 32rpx;
}
#fuOrderDetail .couponGanT .rightCon .pirce {
  font-size: 35rpx;
  margin: 8rpx 0 16rpx;
}
#fuOrderDetail .couponGanT .rightCon .btn {
  margin: 0 20rpx;
  padding: 5rpx 30rpx;
  box-sizing: border-box;
  background: #FF6262;
  color: white;
  border-radius: 20rpx;
}
#fuOrderDetail .couponGanT .middren {
  z-index: 2;
  border-radius: 10rpx;
  position: absolute;
  display: flex;
  align-items: center;
  top: 0;
  left: -40rpx;
  height: 100%;
}
#fuOrderDetail .couponGanT .middren .fenSe {
  padding: 20rpx;
  border-radius: 50%;
  border: 1px solid #fff;
  background: #FF6262;
  position: relative;
}
#fuOrderDetail .couponGanT .middren .fenSe .bg {
  border-radius: 50%;
  background: white;
  width: 40rpx;
  height: 40rpx;
}
#fuOrderDetail .couponGanT .middren .fenSe::before {
  position: absolute;
  content: '';
  top: -5rpx;
  right: calc(20rpx + 1px);
  height: 10rpx;
  width: 20rpx;
  background: #FF6262;
}
#fuOrderDetail .couponGanT .middren .fenSe::after {
  position: absolute;
  content: '';
  bottom: -5rpx;
  right: calc(20rpx + 1px);
  height: 10rpx;
  width: 20rpx;
  background: #FF6262;
}
#fuOrderDetail .btnOD {
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  background: white;
}
#fuOrderDetail .btnOD .money {
  font-size: 30rpx;
  color: #999;
  display: flex;
  align-items: center;
  flex: 1;
}
#fuOrderDetail .btnOD .money .num {
  color: #F56F68;
}
#fuOrderDetail .btnOD .van-button {
  font-size: 28rpx;
  color: #666666;
  border: 1px solid #e9e9e9;
  border-radius: 10rpx;
  background: none;
}
#fuOrderDetail .btnOD .red {
  color: #F56F68;
  font-size: 28rpx;
}
#fuOrderDetail .PJalert .van-popup {
  display: flex;
  align-items: center;
  justify-content: center;
}
#fuOrderDetail .PJalert .van-popup .img {
  width: 75%;
  position: relative;
}
#fuOrderDetail .PJalert .van-popup .img .tip,
#fuOrderDetail .PJalert .van-popup .img .btn {
  position: absolute;
}
#fuOrderDetail .PJalert .van-popup .img .tip {
  text-align: center;
  width: 100%;
  color: #000;
  top: 54%;
  text-decoration: underline;
}
#fuOrderDetail .PJalert .van-popup .img .btn {
  bottom: 2%;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
#fuOrderDetail .PJalert .van-popup .img .btn .child {
  box-shadow: 0 0 50rpx rgba(88, 88, 88, 0.3);
  margin: 0 20rpx;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  background: #f9b552;
  color: #fff;
  font-size: 32rpx;
  border-radius: 30rpx;
}
#fuOrderDetail .PJalert .van-popup .img .btn .wihte {
  background: #fff1dc;
  color: #f27b67;
}
