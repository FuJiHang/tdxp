/* pages/shop/shop.wxss */
.page-section-spacing {
  box-sizing: border-box;
}

.slide-image {
  width: 750rpx;
  display: inline-block;
}

.flex-wrp {
  width: 100%;
  display: flex;
}

.flex-wrp image {
  display: flex;
}

.flex-item {
  width: 25%;
  margin-right: 1px;
  overflow: hidden;
}

.flex-wrp .flex-item:last-child {
  margin-right: 0;
}

.flex-item image {
  display: flex;
  width: 100px;
  height: 100px;
}

.flex-group image {
  display: flex;
}

.flex-group-top {
  width: 100%;
  height: 72px;
  overflow: hidden;
}

.flex-group-top image {
  width: 750px;
  height: 72px;
}

.flex-group-l {
  float: left;
  width: 40%;
  height: 360rpx;
  overflow: hidden;
}

.flex-group-l .pic {
  width: 300px;
}

.flex-group-r {
  float: left;
  width: 60%;
  overflow: hidden;
  height: 360rpx;
}

.page-section {
  overflow: hidden;
}

.page-section .banner-pic {
  overflow: hidden;
}

.page-section .hd-pic {
  overflow: hidden;
}

.page-section .hd-pic image {
  overflow: hidden;
  display: block;
}

.page-section .bd-pic {
  width: 100%;
  height: 360rpx;
  overflow: hidden;
  display: flex;
}

.page-section .bd-pic .bd-l-pic {
  float: left;
  width: 40%;
}

.page-section .bd-pic .bd-r-pic {
  float: right;
  width: 60%;
}

.page-section .bd-pic .bd-r-pic .bd-r-t-pic,
.page-section .bd-pic .bd-r-pic .bd-r-b-pic,
.bd-l-t-pic,
.bd-l-b-pic {
  display: flex;
  height: 180rpx;
  overflow: hidden;
}

.bd-r-b-l-pic {
  float: left;
  width: 50%;
  overflow: hidden;
}

.bd-r-b-r-pic {
  float: right;
  width: 50%;
  overflow: hidden;
}

.page-section .bd-list-b {
  width: 100%;
  height: 180rpx;
  overflow: hidden;
  display: flex;
}

.page-section .bd-list-b .bd-list-b-pic {
  width: 25%;
  float: left;
}

.menu {
  padding: 15rpx 0;
  overflow: hidden;
}

.homecont .menu:last-child {
  margin-bottom: 24rpx;
}

.menu-icon {
  width: 25%;
  float: left;
  text-align: center;
  margin-top: 16rpx;
  margin-bottom: 20rpx;
}

.menu-icon image {
  width: 96rpx !important;
  height: 96rpx !important;
  display: inline-block;
}

.menu-icon .menu-name {
  font-size: 24rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  color: #616161;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.page-section .bd-list-pic {
  width: 100%;
  overflow: hidden;
}

.page-section .bd-list-pic .bd-list-pro-pic {
  width: 50%;
  height: 182rpx;
  float: left;
  overflow: hidden;
  display: flex;
}

.page-section .bd-list-pic .bd-list-pro-pic:nth-child(2n+0) image {
  border-right: 0;
}

.page-section-title {
  /* text-align: center; */
  padding: 24rpx 0;
  /* margin: 0 24rpx; */
  line-height: 48rpx;
  display: flex;
  color: #212121;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  border-bottom: 2rpx solid rgb(240, 240, 240);
}

.page-section-title-shop {
  flex-grow: 1;
  margin-left: 84rpx;
  font-size: 30rpx;

}

.page-section-title-more {
  text-align: right;
  margin-right: 78rpx;
  color: #e0e0e0;
  font-size: 30rpx;

}

.page-section-title-right {
  width: 23rpx;
  height: 23rpx;
  /* background: red; */
  position: absolute;
  right: 26rpx;
  top: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;

}

.page-section-title-right image {
  width: 23rpx;
  height: 23rpx;
}

.fjmdyuan {
  width: 2rpx;
  height: 2rpx;
  background: yellowgreen;
}

.page-section-title .iconfont {
  color: #212121;
  margin-right: 10rpx;
}

.page-section .prodcut-list {
  margin: 0 24rpx;
  width: 702rpx;
}

.page-section .prodcut-list .prodcut-comm {
  margin: 0 -12rpx;
}

.page-section .prodcut-list .prodcut {
  background-color: #fff;
  width: 339rpx;
  overflow: hidden;
  float: left;
  border-bottom: 1rpx solid #e0e0e0;
  padding: 24rpx 12rpx 0 12rpx;
}

.page-section .prodcut-list .prodcut .product-des {
  margin-top: 20rpx;
}

.page-section .prodcut-list .prodcut .product-pic {
  width: 100%;
  height: 320rpx;
  text-align: center;
  overflow: hidden;
}

.page-section .prodcut-list .prodcut .product-pic image {
  vertical-align: middle;
}

.page-section .prodcut-list .prodcut .product-des .product-name {
  font-size: 28rpx;
  line-height: 40rpx;
  height: 80rpx;
  overflow: hidden;
  color: #424242;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.product-into-home {
  height: 48rpx;
  margin-top: 14rpx;
  margin-bottom: 36rpx;
}

.product-add .font48 {
  font-size: 48rpx;
}

.product-into-home .product-price {
  font-size: 32rpx;
  overflow: hidden;
  color: #424242;
  float: left;
  line-height: 36rpx;
  margin-top: 5rpx;
}

.product-into-home .product-shop {
  float: right;
}

.product-into-home .product-shop .product-buy {
  overflow: hidden;
  border: 1px solid #ff8a65;
  border-radius: 20px;
  text-align: center;
  width: 143rpx;
  height: 46rpx;
  line-height: 48rpx;
  padding: 0 10rpx;
}

.product-into-home .product-shop .product-buy .l {
  float: left;
  color: #ff8a65;
  margin-top: -3rpx;
  width: 24rpx;
}

.product-into-home .product-shop .product-buy .r {
  float: right;
  color: #ff8a65;
  width: 24rpx;
  margin-top: -3rpx;
}

.product-into-home .product-shop .product-buy .m {
  width: 95rpx;
  text-align: center;
  line-height: 48rpx;
  font-size: 32rpx;
  float: left;
  color: black;
}

.product-into-home .product-add {
  text-align: center;
  position: relative;
  color: #ff8a65;
}

.product-into-home .product-add .coust {
  position: absolute;
  top: -10rpx;
  right: -8rpx;
  background: #f44336;
  color: #fff;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 100%;
  font-size: 20rpx;
}

.page-section .prodcut-list .product-add {
  float: right;
  text-align: center;
  position: relative;
}

.page-section .product-list .product-add-num {
  position: absolute;
  top: -10rpx;
  right: -10rpx;
  background: #333;
  color: #fff;
  width: 25rpx;
  height: 25rpx;
  line-height: 25rpx;
  border-radius: 100%;
  font-size: 52rpx;
}

.product-price text {
  font-family: arial;
}

.page-section.merchandise {
  background: #fff;
  position: relative;
}

