page {
  height: 100%;
  overflow: hidden;
}
#technician {
  height: 100%;
  overflow: hidden;
  color: #666;
  box-sizing: border-box;
}
#technician ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
#technician .navTopMO {
  display: flex;
  padding: 18rpx 0;
  box-sizing: border-box;
  background: white;
}
#technician .navTopMO .child {
  flex: 1;
  text-align: center;
}
#technician .navTopMO .child .van-button {
  color: #999;
  border: none;
  background: white;
}
#technician .navTopMO .danger .van-button {
  background: #FF6262;
  color: white;
}
#technician .van_order {
  height: 100%;
  color: #999;
  overflow: hidden;
}
#technician .van_order .van-tabs {
  height: 100%;
  overflow: hidden;
}
#technician .van_order .van-tabs__content {
  height: 100%;
  overflow: hidden;
}
#technician .van_order .van-tabs__content .van-tabs__track {
  height: 100%;
  overflow: hidden;
}
#technician .van_order .van-tabs__content .van-tabs__track .van-tab__pane {
  padding-bottom: 44px ;
  height: 100% ;
  box-sizing: border-box;
  overflow: hidden;
}
#technician .van_order .van-tabs__content .van-tabs__track .van-tab__pane scroll-view {
  overflow: auto;
  height: 100%;
  padding-bottom: 100rpx;
  box-sizing: border-box;
}
#technician .van_order .van-tabs__content .van-tabs__track .van-tab__pane scroll-view .noMore {
  font-size: 28rpx;
  color: #999;
  padding: 20rpx;
  text-align: center;
  box-sizing: border-box;
}
#technician .van_order .van-tab--active {
  color: red;
}
#technician .navBarFu {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background: white;
  display: flex;
  padding: 10rpx 0;
  box-sizing: border-box;
  font-size: 26rpx;
  border-top: 1px solid #f0f0f0;
}
#technician .navBarFu .child {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  flex: 1;
  position: relative;
  opacity: 0.6;
}
#technician .navBarFu .child image {
  width: 40rpx;
  height: 40rpx;
}
#technician .navBarFu .child .name {
  margin-top: 5rpx;
}
#technician .navBarFu .black {
  opacity: 1;
}
#technician .navBarFu .max {
  opacity: 1;
}
#technician .navBarFu .max image {
  margin: auto;
  left: 0;
  right: 0;
  bottom: 0;
  position: absolute;
  top: -40rpx;
  width: 100rpx;
  height: 100rpx;
}
#technician .choosePay {
  position: relative;
  z-index: 801;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f5f5f5;
}
#technician .choosePay image {
  width: 100%;
  height: 70rpx;
}
.van-popup {
  background: none !important ;
}
