.add_tic .upload {
  background-color: #fff;
}
.add_tic .upload .up_title {
  font-size: 28rpx;
  color: #333;
  padding: 15rpx 20rpx;
}
.add_tic .upload .pictbox {
  width: 100%;
  display: flex;
  padding: 0 0 15rpx 20rpx;
  position: relative;
  flex-wrap: wrap;
  margin-top: 10rpx;
}
.add_tic .upload .pictbox .evalteimg {
  width: 110rpx;
  height: 110rpx;
  margin-right: 20rpx;
  position: relative;
  border: 1px solid #c8c8c8;
}
.add_tic .upload .pictbox .evalteimg .tu {
  width: 100%;
  height: 100%;
}
.add_tic .upload .pictbox .evalteimg .close {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: -14rpx;
  right: -13rpx;
}
.add_tic .upload .pictbox #img_name {
  position: absolute;
  top: 0;
  left: -99999999999;
  width: 0;
  height: 0;
  font-size: -999999;
}
.add_tic .upload .takepic {
  width: 110rpx;
  height: 110rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.add_tic .upload .takepic .pict {
  width: 100%;
}
.add_tic .content {
  background-color: #fff;
  margin-top: 20rpx;
}
.add_tic .content .input_wrap {
  display: flex;
  align-items: center;
  padding: 15rpx 20rpx;
  font-size: 28rpx;
  color: #333;
  border-top: 1rpx solid #eee;
}
.add_tic .content .input_wrap .name1 {
  flex: 1;
}
.add_tic .content .input_wrap .name_in {
  flex: 5;
}
.add_tic .btn-area {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 60rpx;
}
.add_tic .btn-area .btn {
  width: 90%;
  background-color: red;
  font-size: 28rpx;
  color: #fff;
}
