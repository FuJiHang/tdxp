  justify-content: space-between;
  margin: 30rpx 0 30rpx 0;
}
.content .hearder .acc_content .acc_img2 .accimg {
  border-radius: 50%;
}
.content .hearder .acc_content .magin-r {
  margin-left: -40rpx;
}
.content .btn_wrap {
