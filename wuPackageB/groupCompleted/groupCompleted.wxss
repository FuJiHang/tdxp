}
.content .hearder .acc_content .acc_img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 1rpx dashed #eee;
  position: relative;
}
.content .hearder .acc_content .acc_img::before {
  content: '团长';
  padding: 0 8rpx;
  border-radius: 18rpx;
