.content {
  box-sizing: border-box;
  padding-bottom: 100rpx;
}
.content .hearder {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #fff;
  box-sizing: border-box;
  padding: 30rpx 0;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
}
.content .hearder .title {
  display: flex;
  align-items: center;
}
.content .hearder .title .title_img {
