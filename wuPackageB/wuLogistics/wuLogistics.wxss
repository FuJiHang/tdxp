.goodsInfo {
  box-sizing: border-box;
  padding: 21rpx 26rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.goodsImg {
  position: relative;
  width: 137rpx;
  height: 137rpx;
  margin-right: 16rpx;
}

.goodsImg image {
  width: 137rpx;
  height: 137rpx;
  border-radius: 10rpx;
}

.goodsImg text {
  position: absolute;
  width: 137rpx;
  line-height: 44rpx;
  background-color: rgba(0, 0, 0, 0.6);
  text-align: center;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 0 0 10rpx 10rpx;
  color: #fff;
  font-size: 24rpx;
}

.logStatus {
  color: #FF6067;
  font-size: 30rpx;
}

.logCompany {
  color: #999;
  font-size: 26rpx;
  margin: 10rpx 0;
}

.order {
  color: #999;
  font-size: 26rpx;
}

.empty {
  text-align: center;
  font-size: 28rpx;
  color: #666;
  padding-top: 30%;
  box-sizing: border-box;
}